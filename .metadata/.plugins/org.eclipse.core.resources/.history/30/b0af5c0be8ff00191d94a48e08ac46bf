package fes.aragon.diccionarioarbol;

import fes.aragon.arbolbinario.Nodo;

public class DiccionarioB<T> {

	private Nodo<T> raiz;
	
	public DiccionarioB() {
		// TODO Auto-generated constructor stub
	}
	
	public void insertar(T dato, Alumno datoA) {
		Nodo<T> n = raiz, previo = null;
		while (n != null) {
			previo = n;
			if (n.comparaMenor(dato)) {
				n = n.getHijoDer();
			} else {
				n = n.getHijoIzq();
			}
		}
		if (raiz == null) {
			raiz = new Nodo<T>(dato);
		} else {
			if (previo.comparaMenor(dato)) {
				previo.setHijoDer(new Nodo<T>(dato));
			} else {
				previo.setHijoIzq(new Nodo<T>(dato));
			}
		}
	}
}
