package fes.aragon;

import fes.aragon.dado.Dado;

public class Jugador {
	
	private String nombre;
	private Dado dadoUno = new Dado();
	private Dado dadoDos = new Dado();
	
	private String getNombre() {
		return nombre;
	}
	private Dado getDadoUno() {
		return dadoUno;
	}
	private Dado getDadoDos() {
		return dadoDos;
	}
	
	
}
