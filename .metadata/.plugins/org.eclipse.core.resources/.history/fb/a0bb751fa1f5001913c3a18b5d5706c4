package fes.aragon.clases;

import java.util.ArrayList;

public class Genero {

	private String nombre;
	private int numeroGenero;
	private ArrayList<Ubicacion> ubicacion = new ArrayList<>();
	
	public Genero() {
	}
	private String getNombre() {
		return nombre;
	}
	private void setNombre(String nombre) {
		this.nombre = nombre;
	}
	private int getNumeroGenero() {
		return numeroGenero;
	}
	private void setNumeroGenero(int numeroGenero) {
		this.numeroGenero = numeroGenero;
	}
	private ArrayList<Ubicacion> getUbicacion() {
		return ubicacion;
	}
	private void setUbicacion(ArrayList<Ubicacion> ubicacion) {
		this.ubicacion = ubicacion;
	}
	
	
}
